{
  "type": "Screen",
  "appBar": {
    "type": "AppBar",
    "attributes": {
      "title": "Rupeek Home"
    }
  },
  "body": {
    "children": [
      {
        "type": "TextBanner",
        "attributes": {
          "text": "To enter your pincode, click here!",
          "type": "info",
          "actions": [
            {
              "type": "event",
              "data": {
                "eventName": "Sample Event Name"
              }
