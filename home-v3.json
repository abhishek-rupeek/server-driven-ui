      },
      {
        "type": "Carousel",
        "attributes": {
          "urlList": [
            "https://rupeek-mobileapp.s3.ap-south-1.amazonaws.com/app-assets/ana/images/home/carousel/1_xxhdpi.webp",
            "https://rupeek-mobileapp.s3.ap-south-1.amazonaws.com/app-assets/ana/images/home/carousel/2_xxhdpi.webp",
            "https://rupeek-mobileapp.s3.ap-south-1.amazonaws.com/app-assets/ana/images/home/carousel/3_xxhdpi.webp",
            "https://rupeek-mobileapp.s3.ap-south-1.amazonaws.com/app-assets/ana/images/home/carousel/4_xxhdpi.webp"
          ],
          "height": 320
        }
      },
      {
        "type": "Banner",
        "attributes": {
          "url": "https://rupeek-mobileapp.s3.ap-south-1.amazonaws.com/app-assets/atl/home/home_banner.webp",
          "height": 250
        }
      }
